<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">
	<class name="Race" table="race">
		<id name="raceid" column="raceid">
			<generator class="identity" />
		</id>
		<property name="racename" column="racename" />
		<property name="racetype" column="racetype" />
		<property name="racestate" column="racestate" />
		<property name="raceaddress" column="raceaddress" />
		<property name="introduction" column="introduction" />
		<property name="raceopendate" column="raceopendate" />
	</class>
</hibernate-mapping>
